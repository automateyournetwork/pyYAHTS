    <td>{{ list }}</td>
    <td>{{ ace }}</td>
    <td>{{ data_to_template['acls'][list].aces[ace].actions.forwarding }}</td>
    <td>{{ data_to_template['acls'][list].aces[ace].actions.logging | default ("No Logging") }}</td>
